package com.itba.edu.ar.model;

public class Attribute {

	private Integer id;
	private String name;
	//Deberia ser un array
	private String values;

	public Attribute(Integer id, String name, String values) {
		this.id = id;
		this.name = name;
		this.values = values;
	}
	
	public String getName() {
		return name;
	}
	
	public String getValues() {
		return values;
	}
	
}
